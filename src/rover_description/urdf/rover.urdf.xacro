<?xml version="1.0"?>
<!--    http://wiki.ros.org/urdf/XML/link   -->
<robot name="scarab"
    xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:arg name="is_sim" default="true" />
    <xacro:include filename="$(find rover_description)/urdf/rover_control.xacro" />

    <link name="base_footprint"/>

    <joint name="base_footprint_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link="body_link"/>
        <origin xyz="0 0 0.393" rpy="0 0 0"/>
    </joint>


    <!-- This is the body of the rover -->
    <link name="body_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="15.0"/>
            <inertia ixx="0.5" ixy="0.0" ixz="0.0" iyy="0.7" iyz="0.0" izz="1.0"/>
        </inertial>


        <visual>
            <origin rpy="0 0 1.579795" xyz="0 0.03 0"/>
            <geometry>
                <!--<box size="1 0.6 0.3"/>-->
                <mesh filename="package://rover_description/meshes/base_link.stl" scale="0.001 0.001 0.001" />
            </geometry>
            <material name="gray">
                <color rgba="0.4 0.4 0.4 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 1.579795" xyz="0 0.03 0"/>
            <geometry>
                <mesh filename="package://rover_description/meshes/base_link.stl" scale="0.001 0.001 0.001" />
            </geometry>
        </collision>
    </link>

    <!-- This is the left suspension assembly -->
    <joint name="susLeft_joint" type="fixed">
        <parent link="body_link" />
        <child link="susLeft_link" />
        <origin xyz ="-0.1 -0.17 0.04" rpy="0 0 -1.579795" />
        <axis xyz="1 0 0" />
    </joint>
    <link name="susLeft_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="2.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://rover_description/meshes/suspension_left_link.stl" scale="0.001 0.001 0.001" />
            </geometry>
            <material name="yellow">
                <color rgba="1 1 0 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://rover_description/meshes/suspension_left_link.stl" scale="0.001 0.001 0.001" />
            </geometry>
        </collision>

    </link>

    <!-- This is the right suspension assembly -->
    <joint name="susRight_joint" type="fixed">
        <parent link="body_link" />
        <child link="susRight_link" />
        <origin xyz ="-0.1 0.23 0.04" rpy="0 0 -1.579795" />
        <axis xyz="1 0 0" />
    </joint>
    <link name="susRight_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="2.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://rover_description/meshes/suspension_right_link.stl" scale="0.001 0.001 0.001" />
            </geometry>
            <material name="yellow">
                <color rgba="1 1 0 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://rover_description/meshes/suspension_right_link.stl" scale="0.001 0.001 0.001" />
            </geometry>
        </collision>

    </link>

    <!-- This is the front right wheel of the rover -->
    <joint name="wheel1_joint" type="continuous">
        <parent link="susRight_link" />
        <child link="wheel1_link" />
        <origin xyz ="-0.1 -0.35 -0.3" rpy="0 0 0" />
        <axis xyz="1 0 0" />
    </joint>
    <link name="wheel1_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.004"/>
        </inertial>

        <visual>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
            <material name="black">
                <color rgba="0.01 0.01 0.01 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
        </collision>

    </link>

    <!-- This is the front left wheel of the rover -->
    <joint name="wheel2_joint" type="continuous">
        <parent link="susLeft_link" />
        <child link="wheel2_link" />
        <origin xyz ="0.1 -0.35 -0.3" rpy="0 0 0" />
        <axis xyz="1 0 0" />
    </joint>
    <link name="wheel2_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.004"/>
        </inertial>

        <visual>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
            <material name="black">
                <color rgba="0.01 0.01 0.01 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
        </collision>

    </link>

    <!-- This is the middle right wheel of the rover -->
    <joint name="wheel3_joint" type="continuous">
        <parent link="susRight_link" />
        <child link="wheel3_link" />
        <origin xyz ="-0.19 0.01 -0.3" rpy="0 0 0" />
        <axis xyz="1 0 0" />
    </joint>
    <link name="wheel3_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.004"/>
        </inertial>

        <visual>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
            <material name="black">
                <color rgba="0.01 0.01 0.01 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
        </collision>

    </link>

    <!-- This is the middle left wheel of the rover -->
    <joint name="wheel4_joint" type="continuous">
        <parent link="susLeft_link" />
        <child link="wheel4_link" />
        <origin xyz ="0.19 0.01 -0.3" rpy="0 0 0" />
        <axis xyz="1 0 0" />
    </joint>
    <link name="wheel4_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.004"/>
        </inertial>
        <visual>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
            <material name="black">
                <color rgba="0.01 0.01 0.01 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
        </collision>

    </link>

    <!-- This is the back right wheel of the rover -->
    <joint name="wheel5_joint" type="continuous">
        <parent link="susRight_link" />
        <child link="wheel5_link" />
        <origin xyz ="-0.05 0.35 -0.3" rpy="0 0 0" />
        <axis xyz="1 0 0" />
    </joint>
    <link name="wheel5_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.004"/>
        </inertial>
        <visual>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
            <material name="black">
                <color rgba="0.01 0.01 0.01 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
        </collision>

    </link>

    <!-- This is the back left wheel of the rover -->
    <joint name="wheel6_joint" type="continuous">
        <parent link="susLeft_link" />
        <child link="wheel6_link" />
        <origin xyz ="0.05 0.35 -0.3" rpy="0 0 0" />
        <axis xyz="1 0 0" />
    </joint>
    <link name="wheel6_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.004"/>
        </inertial>
        <visual>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
            <material name="black">
                <color rgba="0.01 0.01 0.01 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="1.579795 0 1.579795" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.123" length="0.12"/>
            </geometry>
        </collision>

    </link>

    <!-- This is the back left wheel of the rover
    <joint name="wheel2_joint" type="continuous">
        <parent link="body_link" />
        <child link="wheel2_link" />
        <origin xyz ="-0.3 0.35 -0.1" rpy="0 0 0" />
        <axis xyz="0 1 0" />
    </joint>
    <link name="wheel2_link">
        <visual>
            <origin rpy="1.579795 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.15" length="0.1"/>
            </geometry>
            <material name="yellow">
                <color rgba="1 1 0 1"/>
            </material>
        </visual>
    </link>
     -->

</robot>