    <!-- This is the back left wheel of the rover -->
    <joint name="wheel6_joint" type="continuous">
        <parent link="susLeft_link" />
        <child link="wheel6_link" />
        <origin xyz ="0.02 0.35 -0.3" rpy="0 0 0" />
